{
  "manifest_version": 2,
  "name": "FromWhereToWhere",
  "version": "0.1",
  "description": "Structure browsing history in threads",
  "permissions": ["history"],
  "icons": {"48": "icon.png"},
  "browser_action": {
     "default_popup": "fromwheretowhere_threads.html",
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": ["eventPage.js"],
    "persistent": false
  }
}
